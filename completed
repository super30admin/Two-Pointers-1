

class Solution {
    public List<List<Integer>> threeSum(int[] nums) {
        Arrays.sort(nums);
        
        List<List<Integer>> arr = new ArrayList<>();
        
        for(int i=0;i<nums.length; i++){
            
            System.out.println(nums[i]);
            System.out.println(nums[i+1]);
            System.out.println(nums[nums.length-1]);    
            
        }
        return arr;
    }
}
